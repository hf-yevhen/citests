{
  "testRunner": "jest",
  "runnerConfig": "e2e/config.json",
  "skipLegacyWorkersInjection": true,
  "apps": {
    "ios": {
      "type": "ios.app",
      "binaryPath": "SPECIFY_PATH_TO_YOUR_APP_BINARY"
    },
    "android": {
      "type": "android.apk",
      "binaryPath": "SPECIFY_PATH_TO_YOUR_APP_BINARY"
    }
  },
  "devices": {
    "simulator": {
      "type": "ios.simulator",
      "device": {
        "type": "iPhone 12"
      }
    },
    "emulator": {
      "type": "android.emulator",
      "device": {
        "avdName": "Pixel_3a_API_30_x86"
      }
    }
  },
  "configurations": {
    "iosRelease": {
      "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/citests.app",
        "build": "xcodebuild -workspace ios/citests.xcworkspace -scheme citests -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -UseModernBuildSystem=YES",
        "type": "ios.simulator",
        "name": "iPhone 12",
        "device": {
          "type": "iPhone 12"
        }
    },
    "iosDebug": {
      "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/citests.app",
        "build": "xcodebuild -workspace ios/citests.xcworkspace -scheme citests -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -UseModernBuildSystem=YES",
        "type": "ios.simulator",
        "name": "iPhone 12",
        "device": {
          "type": "iPhone 12"
        }
    },
    "android": {
      "device": "emulator",
      "app": "android"
    }
  }
}